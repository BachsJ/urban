{
	"name": "urban",
	"type": "dark",
	"colors": {
		"foreground": "#E7E8EB",
		"descriptionForeground": "#6C7986",
		"textPreformat.foreground": "#E7E8EB",
		"scrollbar.shadow": "#FFFFFF00",
		"widget.shadow": "#FFFFFF00",

		"editorCursor.background": "#51C34F",
		"editorCursor.forground": "#FEF935",

		"tab.activeForeground": "#E7E8EB",
		"tab.inactiveForeground": "#596472",
		"tab.inactiveBackground": "#E7E8EB",
		"tab.border": "#E7E8EB",
		"editorGroupHeader.tabsBackground": "#E7E8EB",

		"editor.background": "#292A2F",
		"editor.foreground": "#E7E8EB",
		"editorHoverWidget.background": "#596472",
		"editorSuggestWidget.background": "#E7E8EB",
		"editorSuggestWidget.foreground": "#596472",
		"editor.selectionBackground": "#47515F",
		"editor.lineHighlightBackground": "#363945",
		"editorLineNumber.foreground": "#6C7986",
		"editorLineNumber.activeForeground": "#E7E8EB",
		
		"input.foreground": "#23252B",
		"input.background" : "#E7E8EB",
		"sideBar.foreground": "#23252B",
		"sideBar.background": "#E7E8EB",
		"button.foreground": "#E7E8EB",
		"button.background": "#0A6FFF",
		"button.hoverBackground": "#74B6F6",
		"progressBar.background": "#6543E9",

		"activityBar.foreground": "#23252B",
		"activityBar.background": "#E7E8EB",
		"activityBarBadge.foreground": "#E7E8EB",
		"activityBarBadge.background": "#DE3A3B",

		"notifications.foreground": "#596472",

		"list.inactiveSelectionForeground": "#E7E8EB",
		"list.inactiveSelectionBackground": "#6C7986",
		"list.activeSelectionForeground": "#E7E8EB",
		"list.activeSelectionBackground": "#0A6FFF",
		"list.focusBackground": "#0A6FFF",
		"list.focusForeground": "#E7E8EB",
		"list.highlightForeground": "#E12DA0",
		"list.hoverBackground": "#74B6F6",

		"statusBar.background": "#0A6FFF",
		"statusBar.noFolderBackground": "#0A6FFF",
		"statusBar.debuggingBackground": "#6543E9",
		"statusBar.foreground": "#E7E8EB",

		"gitDecoration.modifiedResourceForeground": "#2CAD68",
		"gitDecoration.deletedResourceForeground": "#FC6A5D",
		"gitDecoration.untrackedResourceForeground": "#D38D5E",
		"gitDecoration.ignoredResourceForeground": "#47515F",
		"gitDecoration.conflictingResourceForeground": "#DE3A3B",
		"gitDecoration.submoduleResourceForeground": "#FEF935"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6C7986",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#FC6A5D",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Numbers & Characters",
			"scope": [
				"constant.numeric", "constant.character", "entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#A4A0F1",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Magenta bold",
			"scope": [
				"keyword",
				"meta.var",
				"storage.modifier",
				"storage.class",
				"storage.type.function",
				"storage.type",
				"variable.language.this",
				"variable.language.self",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#EF82B1",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Magenta normal",
			"scope": [
				"constant.language",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#EF82B1",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"storage.modifier.import",
				"storage.modifier.package",
				"storage.type.java",
				"storage.type.generic",
				"meta.class",
				"support.class",
				"support.function",
				"support.type"
			],
			"settings": {
				"foreground": "#99E8D5",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Green",
			"scope": [
				"variable.other.object",
				"meta.function-call.generic"
			],
			"settings": {
				"foreground": "#ADD681",
				"fontStyle": "normal"
			}
		},
		{
			"name": "White",
			"scope": [
				"punctuation.section",
				"punctuation.bracket",
				"punctuation.separator",
				"punctuation.defintion",
				"punctuation.definition.parameters",
				"keyword.operator",
				"keyword.control.ternary",
				"variable.parameter",
				"variable.other",
				"meta.brace",
				"meta.method.body",
				"storage.type.function.arrow",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#E7E8EB",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#FD8F3F",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Dark-Gray",
			"scope": [
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#47515F",
				"fontStyle": "normal"
			}
		}
	]
}